<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Room.
    -->
    <changeSet id="20221102044433-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="location_id"
                                 baseTableName="room"
                                 constraintName="fk_room__location_id"
                                 referencedColumnNames="id"
                                 referencedTableName="room_config"/>

        <addForeignKeyConstraint baseColumnNames="name_id"
                                 baseTableName="room"
                                 constraintName="fk_room__name_id"
                                 referencedColumnNames="id"
                                 referencedTableName="floor"/>
    </changeSet>
</databaseChangeLog>
